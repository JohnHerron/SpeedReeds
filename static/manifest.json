{
    "manifest_version": 3,
    "name": "SpeedReeds",
    "version": "0.1.0",
    "description": "Reed Quicker!",
    "action": {
        "default_title": "SpeedReeds",
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "textLoad.js"
    },
    "permissions": [
        "storage",
        "contextMenus"
    ],
    "icons": {
        "16": "panda_icon.png",
        "48": "icon2.png"
    }
}